{
	"name": "bun-react-template",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "src/server.tsx",
	"module": "src/server.tsx",
	"scripts": {
		"dev": "bun --hot src/server.tsx",
		"build": "bun build ./src/assets/index.html --outdir=dist --sourcemap --target=browser --minify --define:process.env.NODE_ENV='\"production\"' --env='BUN_PUBLIC_*'",
		"start": "NODE_ENV=production bun src/server.tsx",
		"migrate": "bun run migrate.ts",
		"create-migration": "bun run create-migration.ts"
	},
	"dependencies": {
		"@ga-ut/fetcher": "^0.1.1",
		"@ga-ut/store": "^1.1.4",
		"clsx": "^2.1.1",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11"
	},
	"devDependencies": {
		"@biomejs/biome": "2.0.6",
		"@types/bun": "latest",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"bun-plugin-tailwind": "latest"
	}
}
